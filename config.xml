<globalConfig>
	
	<!-- Configure default plugin & plugins loaded on startup -->
	<pluginConfig>
		<!-- startupPlugins index of default start up plugin (-1 for no plugins) -->
		<defaultPluginIndex>-1</defaultPluginIndex>

		<!-- Plugins to load on start up -->
		<startupPlugins>
			<!-- Example: <pluginFile>(path to graphi plugin file)</pluginFile> -->
		</startupPlugins>

	</pluginConfig>
</globalConfig>
