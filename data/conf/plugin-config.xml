<?xml version="1.0" encoding="UTF-8"?>

<!-- Configure default plugin & plugins loaded on startup -->
<plugin-config>
	<!-- startupPlugins index of default start up plugin (-1 for no plugins) -->
	<default-pluginIndex>-1</default-plugin-index>

	<!-- Default plugin directory path -->
	<plugin-directory>data/plugins</plugin-directory>

	<!-- Plugins to load on start up -->
	<startup-plugins>
		<!-- Example: <plugin-file>(path to graphi plugin file)</plugin-file> -->
		<!--<plugin-file>/home/denker/workspace/projects/graphi-suic-plugin/target/suicideintent-1.0.jar</plugin-file> -->
	</startup-plugins>
</plugin-config>

